/dts-v1/;

#include <arm/gd32e23x.dtsi>
#include <zephyr/dt-bindings/input/input-event-codes.h>

/ {
model = GigaDevice GD32E230C-EVAL;
compatible = gd,gd32e230-eval;

chosen {
zephyr,sram = &sram0;
zephyr,flash = &flash0;
zephyr,console = &usart0;
};

leds {
compatible = gpio-leds;
led2: led_2 {
gpios = <&gpioc 13 0>;
label = LED 2;
};
led3: led_3 {
gpios = <&gpioc 14 0>;
label = LED 3;
};
led4: led_4 {
gpios = <&gpioc 15 0>;
label = LED 4;
};
};

gpio_keys {
compatible = gpio-keys;
wakeup_key: wakeup_key {
label = Wakeup;
gpios = <&gpioa 0 GPIO_ACTIVE_LOW>;
zephyr,code = <INPUT_KEY_WAKEUP>;
};
};

aliases {
led0 = &led2;
led1 = &led3;
led2 = &led4;
sw0 = &wakeup_key;
};
};

&usart0 {
pinctrl-0 = <&usart0_tx_pa9 &usart0_rx_pa10>;
pinctrl-names = default;
current-speed = <115200>;
status = okay;
};

&gpioa {
status = okay;
};

&gpioc {
status = okay;
};

